<launch>
    <node pkg="ros_piper_tts" type="piper_node" name="piper_tts" output="screen">
        <param name="model_dir" value="$(find piper_tts)/models"/>
        <param name="default_model" value="en_US-ryan-low"/>
        <rosparam command="load" file="$(find piper_tts)/config/models.yaml"/>
    </node>

</launch> 