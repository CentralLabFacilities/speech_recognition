<launch>
    <arg name="model" default="$(find ros_nllb)/models/nllb-200-distilled-600M"/>
    <arg name="ct2_model" default="$(find ros_nllb)/models/ct-nllb-200-distilled-600M"/>

    <node pkg="ros_nllb" type="ct2_node" name="ros_nllb" output="screen">
        <param name="model" value="$(arg model)"/>
        <param name="ct2_model" value="$(arg ct2_model)"/>
    </node>

</launch> 